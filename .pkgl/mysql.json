{
    "name": "mysql",
    "script": "/usr/bin/mysqld",
    "error_file": "./log/mysql/err.log",
    "out_file": "./log/mysql/out.log",
    "merge_logs": true,
    "log_date_format": "YYYY/MM/DD HH:mm:ss",
    "min_uptime": "60s",
    "max_restarts": 30,
    "autorestart": true,
    "args": ["--user=root","--console"],
    "exec_interpreter": "",
    "exec_mode": "fork"
  }
  