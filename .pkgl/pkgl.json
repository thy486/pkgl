{
  "name": "pkgl-server",
  "script": "/usr/bin/java",
  "error_file": "./log/err.log",
  "out_file": "./log/out.log",
  "merge_logs": true,
  "log_date_format": "YYYY/MM/DD HH:mm:ss",
  "min_uptime": "60s",
  "max_restarts": 30,
  "autorestart": true,
  "args": ["-jar", "pkgl.jar", "--spring.datasource.url=jdbc:mysql://localhost:3306/PKGL?useUnicode=true&characterEncoding=utf-8&allowMultiQueries=true&serverTimezone=Asia/Shanghai"],
  "exec_interpreter": "",
  "exec_mode": "fork"
}
